{
    "presets": [
        "@babel/preset-env",
        [
        "@babel/preset-react",
        {
            "runtime": "automatic"
        }
        ],
        "@babel/preset-typescript"
    ],
    "plugins": [
        [
        "module-resolver",
        {
            "alias": {
            "@components": "./src/components",
            "@pages": "./src/pages",
            "@store": "./src/store",
            "@utils": "./src/utils"
            }
        }
        ]
    ]
}

